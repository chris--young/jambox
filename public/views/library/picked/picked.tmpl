<table>
  <thead>
    <tr>
      <th>
        <label>Track</label>
        <a href="#"><span class="fa fa-sort"></span></a>
      </th>
      <th>
        <label>Title</label>
        <a href="#"><span class="fa fa-sort"></span></a>
      </th>
      <th>
        <label>Artist</label>
        <a href="#"><span class="fa fa-sort"></span></a>
      </th>
      <th>
        <label>Album</label>
        <a href="#"><span class="fa fa-sort"></span></a>
      </th>
      <th>
        <label>Length</label>
        <a href="#"><span class="fa fa-sort"></span></a>
      </th>
    </tr>
  </thead>
  <tbody>
    <% _.each(_.range(0, 19), function (index) { %>
      <% if (index < tracks.length) { %>
        <tr>
          <td><%- tracks[index].number %></td>
          <td><%- tracks[index].title %></td>
          <td><%- tracks[index].artist %></td>
          <td><%- tracks[index].album %></td>
          <td><%- tracks[index].length %></td>
        </tr>
      <% } else { %>
        <tr class="empty">
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
      <% } %>
    <% }); %>
  </tbody>
</table>
